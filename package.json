{
	"name": "tab-painter",
	"displayName": "TabPainter",
	"description": "Extension that paints the tabs in user defined colors based on the current connection ",
	"publisher": "OEHLER.DEV",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.39.0",
		"azdata": "*"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:tab-painter.helloWorld",
		"onCommand:tab-painter.showCurrentConnection"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "tab-painter.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "tab-painter.showCurrentConnection",
				"title": "Show Current Connection"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"proposedapi": "node installTypings.js"
	},
	"devDependencies": {
		"@types/azdata": "*",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.39.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"azdata-test": "^1.x",
		"eslint": "^6.8.0",
		"glob": "^7.1.7",
		"mocha": "^11.1.0",
		"typescript": "^4.3.5"
	}
}
